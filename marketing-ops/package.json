{
  "name": "one52bar-marketing-ops",
  "version": "1.0.0",
  "description": "ONE52 Bar & Grill Marketing Operations",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:excel": "jest one-52-excell-marketing-app/src/consolidated/shared/excel/__tests__ --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "# Excel Generation": "",
    "generate:spreadsheet": "ts-node one-52-excell-marketing-app/src/exceljs/generate-marketing-spreadsheet.new.ts",
    "version:excel": "ts-node -P src/consolidated/shared/excel/tsconfig.json src/consolidated/shared/excel/version.ts"
  },
  "keywords": [
    "marketing",
    "excel",
    "typescript",
    "one52bar"
  ],
  "author": "AIREINVESTOR.COM",
  "license": "UNLICENSED",
  "dependencies": {
    "exceljs": "^4.4.0",
    "ts-morph": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.30",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
