{
  "name": "my_app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:fixed": "node scripts/fix-css-build.js && next build",
    "build:spa": "next build -output-standalone",
    "start": "next start",
    "lint": "next lint && pnpm run verify-schema",
    "lint:fix": "next lint --fix && pnpm run verify-schema:fix",
    "export": "next build && next export",
    "test-facebook": "ts-node -r tsconfig-paths/register --project tsconfig.json src/scripts/test-facebook.ts",
    "facebook-token": "node src/scripts/facebook-token.js",
    "update-token": "node src/scripts/update-token.js",
    "oauth-url": "node src/scripts/get-app-info.js",
    "verify-permissions": "node src/scripts/verify-permissions.js",
    "fix-facebook": "node src/scripts/get-new-token.js",
    "use-app-secret": "node src/scripts/use-app-secret.js",
    "get-events": "node src/scripts/get-events-directly.js",
    "use-mock-events": "node src/scripts/use-mock-events.js",
    "app-auth": "node src/scripts/try-app-authentication.js",
    "business-app": "node src/scripts/try-business-app.js",
    "test-page-token": "node src/scripts/test-page-token.js",
    "save-page-token": "node src/scripts/save-page-token.js",
    "generate-proof": "node src/scripts/generate-proof.js",
    "diagram": "ts-node src/scripts/generate-diagrams.ts",
    "diagram:watch": "ts-node src/scripts/generate-diagrams.ts --watch",
    "git:clean": "ts-node scripts/clean-git.ts",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "cd prisma && ts-node --project tsconfig.json seed.ts",
    "prisma:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "verify-schema": "node scripts/verify-schema.mjs",
    "verify-schema:fix": "node scripts/verify-schema.mjs --fix",
    "migrate-tappass": "bash scripts/migrate-tappass.sh",
    "generate-schema": "node scripts/generate-schema.mjs",
    "generate-schema:db": "prisma-ts-generator --dir ./src/app --db sqlite",
    "generate-schema:watch": "node scripts/generate-schema.mjs --watch",
    "test-ts2prisma-config": "ts-node src/types/config.ts",
    "test-watcher": "node scripts/test-watcher.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-members": "ts-node src/scripts/check-members.ts"
  },
  "dependencies": {
